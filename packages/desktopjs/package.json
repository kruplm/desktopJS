{
  "name": "@morgan-stanley/desktopjs",
  "title": "desktopJS",
  "description": "Library for abstracting common container hosting",
  "version": "4.0.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/desktop.js",
  "types": "dist/desktopjs.d.ts",
  "type": "module",
  "license": "Apache-2.0",
  "author": "Morgan Stanley",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MorganStanley/desktopJS.git"
  },
  "bugs": {
    "url": "https://github.com/MorganStanley/desktopJS/issues"
  },
  "keywords": [
    "container",
    "desktop wrapper",
    "desktop",
    "openfin",
    "hadouken",
    "electron"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "clean:dist": "rimraf dist",
    "clean:build": "rimraf build",
    "clean": "npm run clean:dist && npm run clean:build",
    "bundle": "rollup -c",
    "build": "npm run clean && tsc && npm run bundle",
    "test": "gulp test",
    "deploy": "npm publish --provenance --access public",
    "prepublishOnly": "copyfiles ../../README.md . -u 2"
  },
  "homepage": "https://github.com/MorganStanley/desktopJS#readme"
}
